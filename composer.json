{
  "name": "urn2/nx",
  "description": "A Trait Framework for PHP",
  "homepage": "https://github.com/urn2/nx",
  "type": "framework",
  "authors": [
    {
      "name": "Vea",
      "homepage": "http://www.f2016.cn",
      "email": "vea@live.cn"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-zip": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-redis": "*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "nx\\": [
        "src/",
        "deprecated/"
      ],
      "nx\\tools\\": "tools/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  }
}
